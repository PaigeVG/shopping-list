<h1>Show List item</h1>

<ul>

  <li>
    <strong>List ID:</strong>
    <%= @list_item.list_id %>
  </li>

  <li>
    <strong>Item:</strong>
    <%= @list_item.item_name %>
  </li>

  <li>
    <strong>Quantity:</strong>
    <%= @list_item.quantity %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.list_list_item_path(@conn, :edit, @list_item.list_id, @list_item) %></span>
<span><%= link "Back", to: Routes.list_list_item_path(@conn, :index, @list_item.list_id) %></span>
